package Day1;

public class Loops {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i=0; i<=5;i++) {
			System.out.println(i);
		}
		System.out.println("-----------------------------------------------------");
		int num=1;
		while(num<=5) {
			System.out.println( num);
			num++;
		}

	}

}
