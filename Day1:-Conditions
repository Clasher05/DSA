package Day1;

public class Conditions {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num=5;
		if ( num> 0) {
			System.out.println("positive");
		}
			else if (num<0) {
				System.out.println("Negative");
			}
			else {
				System.out.println("zero");
			}
		
	}

}

