package Day1;

import java.util.*;
public class Arrays_concept {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] arr= {5,1,2,3,4};
		System.out.println(arr[0]);
		
		
		System.out.println("-----------------------------------");
		// for each 
		for(int num:arr) {
			System.out.println(num);
		}
		
		System.out.println("------------------------------");
		Arrays.sort(arr);
		System.out.println(Arrays.toString(arr));

	}

}
